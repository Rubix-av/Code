public class Main{
    public static void main(String[] args){

        int x = 123;
        double y = 3.14;
        boolean z = true;
        char symbol = 'A';
        String name = "Akshobh";
        System.out.println("My Number is: "+x);
        System.out.println("My Number is: "+y);
        System.out.println("My Bool is: "+z);
        System.out.println("My char is: "+symbol);
        System.out.println("Hello"+name);

        Double doubleObject = y;
        
        System.out.println(doubleObject.getClass());

    }

}